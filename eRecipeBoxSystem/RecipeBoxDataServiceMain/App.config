<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<system.serviceModel>
		<services>
			<!--  use this service for WCF with username and pw -->
			<service name="RecipeBoxDataService.Service" behaviorConfiguration="UserNamePasswordBehavior">
				<endpoint name="wsSecureService" address="" binding="wsHttpBinding" bindingConfiguration="wsSecureBindingConfiguration" contract="DevExpress.Xpo.DB.IDataStoreService" />
				<host>
					<baseAddresses>
						<add baseAddress="https://localhost:5757/service" />
					</baseAddresses>
				</host>
			</service>



			<!--  use this sevice for basic http.  development only since it is not secure.
      <service name="RecipeBoxDataService.Service" behaviorConfiguration="RecipeBoxDataService.ServiceBehavior">
        <endpoint address="" binding="basicHttpBinding" bindingConfiguration="ServicesBinding" contract="DevExpress.Xpo.DB.IDataStoreService">          
			<identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:5700/service" />
          </baseAddresses>
        </host>
      </service> -->

		</services>
		<bindings>
			<wsHttpBinding>
				<binding name="wsSecureBindingConfiguration" maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647">
					<readerQuotas maxDepth="2147483647" maxArrayLength="2147483647" maxStringContentLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
					<security mode="TransportWithMessageCredential">
						<message clientCredentialType="UserName" />
					</security>
				</binding>
			</wsHttpBinding>
			<basicHttpBinding>
				<binding name="ServicesBinding" maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647" maxBufferSize="2147483647" transferMode="Streamed">
					<readerQuotas maxDepth="2147483647" maxArrayLength="2147483647" maxStringContentLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
				</binding>
			</basicHttpBinding>
		</bindings>
		<behaviors>
			<serviceBehaviors>
				<behavior name="UserNamePasswordBehavior">
					<!-- To avoid disclosing metadata information, set the values below to false before deployment -->
					<serviceMetadata httpGetEnabled="false" httpsGetEnabled="false" />
					<!-- To receive exception details in faults for debugging purposes, set the value below to true.  Set to false before deployment to avoid disclosing exception information -->
					<serviceDebug includeExceptionDetailInFaults="false" />
					<!--3. Add This Tag to Know Security-->
					<serviceCredentials>
						<userNameAuthentication userNamePasswordValidationMode="Custom" customUserNamePasswordValidatorType="RecipeBoxDataService.CustomUserNameValidator, RecipeBoxDataService" />
					</serviceCredentials>
				</behavior>

				<behavior name="RecipeBoxDataService.ServiceBehavior">
					<serviceMetadata httpGetEnabled="true" />
					<serviceDebug includeExceptionDetailInFaults="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true" />
		<directoryBrowse enabled="true" />
	</system.webServer>	
	<connectionStrings>
		<add name="Recipes" connectionString="XpoProvider=MSSqlServer;Server=localhost;initial catalog=recipeTest1;Trusted_Connection=True" />
		<add name="RecipesLanSQLServer" connectionString="XpoProvider=MSSqlServer;Data Source=192.168.1.88;initial catalog=recipeTest1;user id=sa;password=700A7AB01B51AB4F005BFF735FFAE00A;Persist Security Info=true" />
		<add name="RecipesPG" connectionString="XpoProvider=Postgres;Host=localhost;Username=postgres;Password=yourpw;Database=recipeTest1;Encoding=UNICODE;Persist Security Info=true" />
	</connectionStrings>
